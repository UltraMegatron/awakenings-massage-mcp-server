{
  "version": 2,
  "name": "awakenings-canva-sync",
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@latest"
    }
  },
  "rewrites": [
    {
      "source": "/privacy-policy",
      "destination": "/privacy-policy.html"
    },
    {
      "source": "/support",
      "destination": "/support.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}